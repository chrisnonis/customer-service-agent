{
  "version": 2,
  "builds": [
    {
      "src": "ui/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "ui/$1"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "GOOGLE_CSE_ID": "@google_cse_id",
    "GOOGLE_API_KEY": "@google_api_key"
  }
} 